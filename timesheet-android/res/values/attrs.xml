<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="itemType">
        <enum name="text" value="0" />
        <enum name="toggle" value="1" />
    </attr>
    
    <declare-styleable name="common_attr">
        <attr name="labelText" format="string"/>
        <attr name="isBold" format="boolean"/>
        <attr name="hasHeader" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SettingItem">
        <attr name="labelText"/>
        <attr name="itemType"/>
    </declare-styleable>

    <declare-styleable name="WeekDayView">
        <attr name="labelText"/>
    </declare-styleable>

    <declare-styleable name="TitleBar">
        <attr name="labelText"/>
    </declare-styleable>

    <declare-styleable name="ContactAgent">
        <attr name="labelText"/>
        <attr name="android:enabled"/>
        <attr name="android:inputType"/>
        <attr name="android:nextFocusDown"/>
        <attr name="android:nextFocusUp"/>
    </declare-styleable>

    <declare-styleable name="SearchBar">
        <attr name="android:editable"/>
        <attr name="android:hint"/>
    </declare-styleable>

    <declare-styleable name="ReaTextView">
        <attr name="isBold"/>
    </declare-styleable>

    <declare-styleable name="RefineSearchItemWithCheckbox">
        <attr name="labelText"/>
    </declare-styleable>

    <declare-styleable name="BookmarkButton">
        <attr name="isNavigateButton" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ReaListView">
        <attr name="hasHeader"/>
    </declare-styleable>

    <declare-styleable name="ReaMapView">
        <attr name="hasHeader"/>
    </declare-styleable>

    <declare-styleable name="EcoGallery">
        <attr name="gravity" format="integer"/>
        <attr name="animationDuration" format="integer"/>
        <attr name="unselectedAlpha" format="float"/>
        <attr name="spacing" format="dimension"/>
    </declare-styleable>

    <attr name="ecoGalleryStyle" format="reference"/>
    
    <declare-styleable name="CustomAbsSpinner">
        <attr name="entries" format="reference"/>
    </declare-styleable>

    <attr name="customAbsSpinnerStyle" format="reference"/>
</resources>